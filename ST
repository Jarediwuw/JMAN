		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("", "System")
		local x = require(game.Players.LocalPlayer.PlayerScripts.ChatScript.ChatMain)
		x.MessagePosted:fire(".unlock")
		warn(".unlock")
end)
