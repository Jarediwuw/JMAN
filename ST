    if View.Text == 'View' then
        View.Text = 'Unview'
        repeat view(TargetTextbox.Text) until View.Text == 'View'
        game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject = player.Character:FindFirstChildWhichIsA('Humanoid')
    else
        View.Text = 'View'
    end
end)
