function hideframes()
    MainFrame.Visible = false
    QuickTpFrame.Visible = false
    SideScriptsFrame.Visible = false
    TogglesFrame.Visible = false
    SellingFrame.Visible = false
    imiecreditsFrame.Visible = false
end

function announce(title,text,time)
    game.StarterGui:SetCore("SendNotification", {
        Title = title;
        Text = text;
        Icon = "http://www.roblox.com/asset/?id=8826036839";
        Duration = time;
    })
   
local cor = coroutine.wrap(function()
	local sound = Instance.new("Sound", game.CoreGui)
	sound.SoundId = "rbxassetid://4590656842"
	sound:Play()
	wait(4)
	sound:Destroy()
end)
cor()
end

